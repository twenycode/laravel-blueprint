{
  "name": "twenycode/laravel-blueprint",
  "description": "Core components and utilities for Laravel applications",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "TWENY LIMITED",
      "email": "twenycode@tweny.co.tz"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0",
    "vinkla/hashids": "^13.0",
    "realrashid/sweet-alert": "^7.3",
    "twenycode/blade-morph": "^1.4",
    "spatie/laravel-activitylog": "^4.10",
    "spatie/laravel-permission": "^6.21",
    "rap2hpoutre/laravel-log-viewer": "^2.4",
    "rappasoft/laravel-authentication-log": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6",
    "barryvdh/laravel-debugbar": "^3.15",
    "beyondcode/laravel-query-detector": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "TwenyCode\\LaravelBlueprint\\": "src/"
    },
    "files": [
      "helpers/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "TwenyCode\\LaravelBlueprint\\TwenyLaravelBlueprintServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TwenyCode\\LaravelBlueprint\\Tests\\": "tests/"
    }
  }
}