{
  "name": "twenycode/laravel-blueprint",
  "description": "Core components and utilities for Laravel applications",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "TWENY LIMITED",
      "email": "twenycode@tweny.co.tz"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0|^12.0",
    "vinkla/hashids": "^12.0",
    "realrashid/sweet-alert": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "TwenyCode\\LaravelBlueprint\\": "src/"
    },
    "files": [
      "helpers/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "TwenyCode\\LaravelBlueprint\\TwenyLaravelBlueprintServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TwenyCode\\LaravelBlueprint\\Tests\\": "tests/"
    }
  }
}